maior, menor, media = 0, 0, 0
t = []
for i in range(101):
    n = float(input())
    t.append(n)
    if 15 > n > 40:
        break
    media += t[i]
    if i == 0:
        maior = menor = n
    if t[i] > maior:
        maior = t[i]
    if menor > t[i]:
        menor = t[i]
for i in range(101):
    if t[i] > (media/101):
        print(t[i])
print(t)
print(menor)
print(maior)
print(media/101)
